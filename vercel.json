{
    

    "builds": [{
        "src": "UNIVERSITY_ADMISSION/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb",
             "runtime": "python3.9"
            
             }
       
          
    }],
   
    "routes": [
    
        {
            "src": "/(.*)",
            "dest": "UNIVERSITY_ADMISSION/wsgi.py"
        }
    ]
}